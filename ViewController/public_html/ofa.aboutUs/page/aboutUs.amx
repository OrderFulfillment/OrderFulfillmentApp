<?xml version="1.0" encoding="UTF-8" ?>
<amx:view xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:amx="http://xmlns.oracle.com/adf/mf/amx"
          xmlns:dvtm="http://xmlns.oracle.com/adf/mf/amx/dvt">
    <amx:panelPage id="pp1">
        <amx:facet name="header">
            <amx:panelGroupLayout id="pgl2" layout="horizontal" halign="center" valign="middle">
                <amx:outputText value="About Us" id="ot1" inlineStyle="color:White;width: 290px;text-align: center;"/>
            </amx:panelGroupLayout>
        </amx:facet>
        <amx:facet name="primary">
            <amx:commandLink id="cl3" shortDesc="Settings" action="__back">
                <amx:outputHtml id="oh1" styleClass="fa fa-chevron-left fa-lg" inlineStyle="color:white;"/>
            </amx:commandLink>
        </amx:facet>
        <amx:panelStretchLayout id="psl1">
            <amx:facet name="top"/>
            <amx:facet name="center">
                <amx:listView showDividerCount="true" var="row" showMoreStrategy="autoScroll" id="Tab1"
                              bufferStrategy="viewport"
                              inlineStyle="background-color:white; margin-top:0px; margin:10px; text-align:justify;"
                              xmlns:amx="http://xmlns.oracle.com/adf/mf/amx">
                    <amx:listItem id="li2">
                        <amx:verbatim id="v1">The Dashboard provides a detailed insight into the overall fulfillment process and enables the concerned fulfillment manager to take corrective actions on the move for orders which bear the risk of getting delayed, based on the alerts/notifications generated by the application.</amx:verbatim>
                    </amx:listItem>
